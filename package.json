{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --target node",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rx-tracker",
  "author": "Paruyr",
  "module": "dist/rx-tracker.esm.js",
  "size-limit": [
    {
      "path": "dist/rx-tracker.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rx-tracker.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/warning": "^3.0.0",
    "husky": "^7.0.4",
    "reflect-metadata": "^0.1.13",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "tiny-invariant": "^1.2.0"
  }
}
